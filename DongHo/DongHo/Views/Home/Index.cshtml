
@{
    ViewBag.Title = "Rolex";
}
<link href="~/Content/Quang.css" rel="stylesheet" />
<div class="container">
    <br>
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
            <li data-target="#myCarousel" data-slide-to="3"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">

            <div class="item active">
                <img src="~/image/baner/slide.jpg" alt="Chania" width:"100%" height="345">
                <div class="carousel-caption">
                    <h3>Đồng Hồ Rolex</h3>
                    <p>Không chỉ là dông hồ, đó còn là một đẳng cấp</p>
                </div>
            </div>

            <div class="item">
                <img src="~/image/baner/slide2.jpg" alt="Chania" width:"100%" height="345">
                <div class="carousel-caption">
                    <h3>Rolex King</h3>
                    <p>Một đột phá của 2016</p>
                </div>
            </div>

            <div class="item">
                <img src="~/image/baner/slide3.jpg" alt="Flower" width:"100%" height="345">
                <div class="carousel-caption">
                    <h3>Sale Khủng</h3>
                    <p>Chào đón 20/10 Rolex bắt đầu giảm giá</p>
                </div>
            </div>

            <div class="item">
                <img src="~/image/baner/slide4.jpg" alt="Flower" width:"100%" height="345">
                <div class="carousel-caption">
                    <h3>Thần tượng ưu thích  Rolex</h3>
                    <p>Thống kê thương hiệu yêu thích</p>

                </div>
            </div>
        </div>
    </div>
</div>

