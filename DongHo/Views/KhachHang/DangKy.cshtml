@model DongHo.Models.KHACHHANG
@{
    ViewBag.Title = "Đăng Ký";
    Layout = "~/Views/Shared/_LayoutHome.cshtml";
}

<html>
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Đăng Ký</title>

    <!-- CSS -->
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Roboto:400,100,300,500">
    <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/form-elements.css">
    <link rel="stylesheet" href="~/Content/style.css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Favicon and touch icons -->
    <link rel="shortcut icon" href="assets/ico/favicon.png">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="assets/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="assets/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="assets/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="assets/ico/apple-touch-icon-57-precomposed.png">

</head>

<body>

    <!-- Top content -->
    <div class="Login">

        <div class="inner-bg">
            <div class="container">
                <div class="row">
                    <div class="col-sm-8 col-sm-offset-2 text">
                        <h1><strong></strong></h1>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-6 col-sm-offset-3 form-box">
                        <div class="form-top">
                            <div class="form-top-left">
                                <h3 style="color:#ffffff;text-align:center">Màn Hình Đăng Ký</h3>
                                <p style="text-align:center">Mời bạn nhập đầy đủ thông tin đê đăng ký</p>
                            </div>
                        </div>
                        <div class="form-bottom">
                            <form role="form" action="" method="post" class="login-form">
                                <div class="form-group">
                                    <label class="sr-only" for="form-username">Họ Tên</label>
                                    <input type="text" name="name" placeholder="Họ Tên..." class="form-username form-control" id="form-username">
                                    
                                </div>
                                <div class="form-group">
                                    <label class="sr-only" for="form-password">Số Diện Thoại</label>
                                    <input type="text" name="phonenumble" placeholder="Số Điện thoại..." class="form-password form-control" id="form-password">
                                   
                                </div>
                                <div class="form-group">
                                    <label class="sr-only" for="form-password">Địa Chỉ</label>
                                    <input type="text" name="DiaChi" placeholder="Địa Chỉ" class="form-password form-control" id="form-password">

                                </div>
                                <div class="form-group">
                                    <label class="sr-only" for="form-password">Mật Khẩu</label>
                                    <input type="password" name="password" placeholder="Mật Khẩu..." class="form-password form-control" id="form-password">
                                </div>
                                <div class="form-group">
                                    <label class="sr-only" for="form-password">Nhập Lại Mật Khẩu</label>
                                    <input type="password" name="password2" placeholder="Nhập Lại Mật Khẩu..." class="form-password form-control" id="form-password">
                                    
                                </div>
                                <button type="submit" style="position:center" class="btn">Đăng Ký</button>
                            </form>

                        </div>
                    </div>
                </div>
                
            </div>
        </div>

    </div>


    <!-- Javascript -->
    <script src="~/scripts/jquery-1.11.1.min.js"></script>
    <script src="~/scripts/jquery-1.11.1.min.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
    <script src="~/scripts/jquery.backstretch.min.js"></script>
    <script src="~/scripts/scripts.js"></script>

    <!--[if lt IE 10]>
        <script src="~/scripts/placeholder.js"></script>
    <![endif]-->

</body>

</html>




